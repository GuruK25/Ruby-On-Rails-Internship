<div id="<%= dom_id customer %>">
  <div>
    <strong>Id:</strong>
    <%= customer.id %>
  </div>

  <div>
    <strong>Name:</strong>
    <%= customer.name %>
  </div>

  <div>
    <strong>Email:</strong>
    <%= customer.email %>
  </div>

  <div>
    <strong>About me</strong>
    <%= customer&.about_me || "-" %>
  </div>

  <div>
    <strong>Date of birth </strong>
    <%= customer.dob %>
  </div>

</div>
